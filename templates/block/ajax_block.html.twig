{{ block.datas | dataRender(timeline, numweek, year, day, tabValeurs) | raw }}

<script type="text/javascript">
	$(document).ready(function() {
		var params = {};
	    	Block.Launch(params);

	    	{% if(timeline == '1s') %}
	    		Block.Event1s('#block-time-{{block.id}}');
	    	{% elseif (timeline == '1j') %}
	    		Block.Event1j('#block-time-{{block.id}}');
	    	{% endif %}
        });
</script>